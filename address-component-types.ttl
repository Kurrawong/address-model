addrct:anz-components-order
    a skos:OrderedCollection ;
    dcterms:identifier "anz-components-order"^^xsd:token ;
    skos:prefLabel "ANZ Components Order" ;
    skos:definition "The list of Address Components in address printing order use in Australia and New Zealand"@en ;
