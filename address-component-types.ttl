cs:
    a skos:ConceptScheme ;
    dcterms:identifier "address-component-types"^^xsd:token ;
    dcterms:created "2022-05-04"^^xsd:date ;
    dcterms:creator <https://orcid.org/0000-0002-8742-7730> ;
    dcterms:modified "2022-05-04"^^xsd:date ;
    dcterms:publisher <https://orcid.org/0000-0002-8742-7730> ;
    dcterms:rights "Copyright 2022, Nicholas J. Car" ;
    dcterms:provenance "Developed for the ANZ National Address Model using the table AddressComponentType from ISO19160-1 as a partial source" ;
    skos:definition "Address Component Types are kinds of Address Components. The kind is used to validate Address Component values and to order Address Components for address printing."@en ;
    skos:hasTopConcept
        addrct:addressedObjectIdentifier ,
        addrct:administrativeAreaName ,
        addrct:countryCode ,
        addrct:countryName ,
        addrct:localityName ,
        addrct:postcode ,
        addrct:postOfficeName ,
        addrct:thoroughfareName ;
    skos:prefLabel "Address Component Types" ;
.

<https://orcid.org/0000-0002-8742-7730>
    a sdo:Person ;
    sdo:name "Nicholas J. Car" ;
    sdo:email "nick@kurrawong.net"^^xsd:anyURI ;
.

addrct:iso19160-1-components
    a skos:OrderedCollection ;
    dcterms:identifier "iso19160-1-components"^^xsd:token ;
    skos:prefLabel "ISO19160-1 Address Components" ;
    skos:definition "The list of Address Component Types presented in the original ISO19160-1 standard's codelist"@en ;
    skos:member
        addrct:addressedObjectIdentifier ,
        addrct:administrativeAreaName ,
        addrct:countryCode ,
        addrct:countryName ,
        addrct:localityName ,
        addrct:postcode ,
        addrct:postOfficeName ,
        addrct:thoroughfareName ;
    dcterms:provenance "The list of Address Component Types presented in the original ISO19160-1 standard's codelist"@en ;
.

addrct:anz-additional-address-components
    a skos:Collection ;
    dcterms:identifier "anz-additional-address-components"^^xsd:token ;
    skos:prefLabel "ANZ Additional Address Components" ;
    skos:definition "The list of additional Address Component Types on top of the ISO19160-1 standard created for use in Australia and New Zealand"@en ;
    skos:member
        addrct:recipientName ,
        addrct:streetNumber ,
        addrct:poBox ,
        addrct:traditionalPlaceName ;
    dcterms:provenance "Created for the ANZ National Address Model - Candidate"@en ;
