PREFIX addr: <https://w3id.org/profile/anz-address/>
PREFIX cad: <https://w3id.org/profile/cad/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX isov1: <http://def.isotc211.org/iso19160/-1/2015/Address/code/AddressComponentType/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<http://example.com/oxford>
    a addr:Address ;
    addr:hasAddressComponent
        [
            addr:hasComponentType isov1:numberFirst ;
            addr:hasTextValue "20" ;
            addr:hasValue 20
        ] ,
        [
            addr:hasComponentType isov1:streetLocality ;
            addr:hasTextValue "Oxford Place" ;
            addr:hasValue <https://linked.data.gov.au/dataset/gnaf/streetLocality/QLD140492>
        ] ,
        [
            addr:hasComponentType isov1:locality ;
            addr:hasTextValue "Shorncliffe" ;
            addr:hasValue <https://linked.data.gov.au/dataset/gnaf/locality/loc38f189794e03>
        ] ,
        [
            addr:hasComponentType isov1:administrativeArea ;
            addr:hasTextValue "Queensland" ;
            addr:hasValue <https://linked.data.gov.au/dataset/asgs/STE/3>
        ] ,
        [
            addr:hasComponentType isov1:postcode ;
            addr:hasTextValue "4017" ;
            addr:hasValue 4017
        ] ;
    addr:hasGeocode [
            dcterms:type <https://linked.data.gov.au/dataset/gnaf/code/geocodeType/PC> ;  # Property Centroid
            geo:asWKT "POINT(153.08468369 -27.32703398)"^^geo:wktLiteral
        ] ;
    addr:isAddressFor <https://linked.data.gov.au/dataset/gnaf/addressSite/161162441> ;
    addr:isAliasOf
        [
            dcterms:type <https://linked.data.gov.au/dataset/gnaf/code/addressAliasType/SYN> ;
            rdf:value <https://linked.data.gov.au/dataset/gnaf/address/GAQLD161268186> ;
        ] ;
.

