:buildingAccessPoint 
    a skos:Concept ;
    dcterms:identifier "buildingAccessPoint"^^xsd:token ;
    dcterms:provenance "Presented in the original ISO19160-1 standard's Annex C Sample Profiles within the AddressPositionType codelist"@en ;
    reg:status status:addition ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The address that identifies the place to access a building AddressableObject from" ;
    skos:inScheme cs: ;
    skos:prefLabel "building access point"@en ;
    skos:topConceptOf cs: ;
.

:centroid
    a skos:Concept ;
